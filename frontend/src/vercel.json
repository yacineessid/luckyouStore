{
    "rewrites": [
      { "source": "/api/users/(.*)", "destination": "/api/users/$1" },
      { "source": "/api/uploads", "destination": "/api/uploads" },
      { "source": "/api/orders/(.*)", "destination": "/api/orders/$1" },
      { "source": "/api/products/(.*)", "destination": "/api/products/$1" },
      { "source": "/uploads/(.*)", "destination": "/uploads/$1" },
      { "source": "/(.*)", "destination": "/" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
          { "key": "Access-Control-Allow-Headers", "value": "X-Requested-With, Content-Type, Accept" }
        ]
      }
    ]
  }
  